<?xml version="1.0" encoding="UTF-8"?>
<description:Group xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:description="http://www.eclipse.org/sirius/description/1.1.0" xmlns:description_1="http://www.eclipse.org/sirius/diagram/description/1.1.0" xmlns:style="http://www.eclipse.org/sirius/diagram/description/style/1.1.0" xmlns:tool="http://www.eclipse.org/sirius/diagram/description/tool/1.1.0" xmlns:tool_1="http://www.eclipse.org/sirius/description/tool/1.1.0" name="qml" version="12.0.0.2017041100">
  <ownedViewpoints name="FeatureModels" modelFileExtension="qml">
    <ownedRepresentations xsi:type="description_1:DiagramDescription" name="FeaturesDiagram" domainClass="qml::FeatureModel" enablePopupBars="true">
      <metamodel href="http://www.example.org/qml#/"/>
      <defaultLayer name="BaseLayer">
        <nodeMappings name="Explainability" domainClass="qml::Explainability">
          <style xsi:type="style:EllipseNodeDescription" labelSize="10" labelExpression="aql:'Explainability [' + self.explainabilityLevel + ']'" sizeComputationExpression="6" labelPosition="node" resizeKind="NSEW">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_yellow']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="Privacy" domainClass="qml::Privacy">
          <style xsi:type="style:EllipseNodeDescription" labelSize="10" labelExpression="Privacy" sizeComputationExpression="6" labelPosition="node" resizeKind="NSEW">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_blue']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="Prediction Correctness" domainClass="qml::PredictionCorrectness">
          <style xsi:type="style:EllipseNodeDescription" labelSize="10" labelExpression="Prediction Correctness" sizeComputationExpression="6" labelPosition="node" resizeKind="NSEW">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_chocolate']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="Fairness" domainClass="qml::Fairness">
          <style xsi:type="style:EllipseNodeDescription" labelExpression="aql: 'Fairness: '+ self.type" sizeComputationExpression="7" labelPosition="node" resizeKind="NSEW">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_green']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="QualityRequirements" domainClass="qml::QualityRequirements">
          <style xsi:type="style:EllipseNodeDescription" labelSize="12" labelExpression="aql:'Quality Requirement \n'+self.task " sizeComputationExpression="10" labelPosition="node" resizeKind="NSEW">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='dark_orange']"/>
          </style>
        </nodeMappings>
        <edgeMappings name="ChildrenEdge" sourceMapping="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@containerMappings[name='FeatureContainer']" targetMapping="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@containerMappings[name='FeatureContainer']" targetFinderExpression="feature:children">
          <style sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            <centerLabelStyleDescription labelSize="10" showIcon="false">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <edgeMappings name="Involved features" sourceMapping="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@nodeMappings[name='Explainability'] //@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@nodeMappings[name='Privacy'] //@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@nodeMappings[name='Prediction%20Correctness'] //@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@nodeMappings[name='Fairness']" targetMapping="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@containerMappings[name='FeatureContainer']" targetFinderExpression="feature:involvedFeature">
          <style lineStyle="dash_dot" sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='orange']"/>
            <centerLabelStyleDescription labelSize="10" showIcon="false" labelExpression="Involved feature">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <edgeMappings name="Implemented arrow" sourceMapping="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@nodeMappings[name='Fairness'] //@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@nodeMappings[name='Privacy'] //@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@nodeMappings[name='Prediction%20Correctness'] //@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@nodeMappings[name='Explainability']" targetMapping="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@containerMappings[name='FeatureContainer']" targetFinderExpression="aql:self.implementedBy">
          <style lineStyle="dash_dot" sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='green']"/>
            <centerLabelStyleDescription labelSize="10" showIcon="false" labelExpression="Implemented by">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <edgeMappings name="QualityRequirementEdge" sourceMapping="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@nodeMappings[name='QualityRequirements']" targetMapping="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@nodeMappings[name='Explainability'] //@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@nodeMappings[name='Fairness'] //@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@nodeMappings[name='Prediction%20Correctness'] //@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@nodeMappings[name='Privacy']" targetFinderExpression="aql:self.relatedTo" sourceFinderExpression="aql:self.eContainer()" domainClass="qml::QualityRequirement" useDomainElement="true">
          <style lineStyle="dash" sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='dark_red']"/>
            <centerLabelStyleDescription labelSize="12">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <edgeMappings name="ThresholdEdge" sourceMapping="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@nodeMappings[name='Fairness'] //@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@nodeMappings[name='Explainability'] //@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@nodeMappings[name='Prediction%20Correctness'] //@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@nodeMappings[name='Privacy']" targetMapping="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@containerMappings[name='FeatureContainer']" targetFinderExpression="aql:self.relatedTo.implementedBy" sourceFinderExpression="aql:self.eContainer().relatedTo" domainClass="qml::Threshold" useDomainElement="true">
          <style lineStyle="dot" sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='dark_chocolate']"/>
            <centerLabelStyleDescription labelSize="10" showIcon="false" labelExpression="aql: 'Threshold: ' + if self.equalityOperator.toString() = 'LOWER_OR_EQUAL' then '&lt;=' else if self.equalityOperator.toString() = 'EQUAL' then '=' else '>=' endif endif + ' ' + self.value">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <containerMappings name="FeatureContainer" labelDirectEdit="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@toolSections.0/@ownedTools[name='editFeatureName']" domainClass="qml::Feature" childrenPresentation="List">
          <subNodeMappings name="openAttribute" semanticCandidatesExpression="feature:attribute" domainClass="qml::OpenValueAttribute">
            <style xsi:type="style:SquareDescription" labelExpression="aql:self.name + ' ['+self.type+']'" labelAlignment="LEFT" labelPosition="node" resizeKind="NSEW">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            </style>
          </subNodeMappings>
          <subNodeMappings name="selectionAttribute" semanticCandidatesExpression="feature:attribute" domainClass="qml::SelectionValueAttribute">
            <style xsi:type="style:SquareDescription" labelExpression="aql:self.name+': '+self.attributevalue.value->sep(', ')->toString()" labelAlignment="LEFT" labelPosition="node" resizeKind="NSEW">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            </style>
          </subNodeMappings>
          <subNodeMappings name="ImplementedQuality" semanticCandidatesExpression="aql:self.oclAsType(qml::Feature).eInverse('implementedBy')" domainClass="qml::QualityProperty">
            <style xsi:type="style:SquareDescription" borderSizeComputationExpression="10" labelExpression="aql:'Implements: '+self.oclAsType(qml::QualityProperty).eClass().name" labelAlignment="LEFT" labelPosition="node" resizeKind="NSEW">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelFormat>italic</labelFormat>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            </style>
          </subNodeMappings>
          <subNodeMappings name="ImplementedMeasure" semanticCandidatesExpression="aql:self.oclAsType(qml::Feature).eInverse('implementedBy')" domainClass="qml::Metric">
            <style xsi:type="style:SquareDescription" labelExpression="aql:'Implements: '+self.name + ' [' + self.eInverse('measure').eClass().name.toUpper() + ']'" labelPosition="node" resizeKind="NSEW">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelFormat>italic</labelFormat>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            </style>
          </subNodeMappings>
          <subNodeMappings name="RequireConstraint" semanticCandidatesExpression="aql:self.oclAsType(qml::Feature).eInverse('requiringFeature')" domainClass="qml::RequireConstraint">
            <style xsi:type="style:SquareDescription" labelExpression="aql:'Require: ' + self.requiredFeature.name->sep(', ')->toString() + ' ' + self.requiredAttribute.name + ' ' + self.requiredAttributeValue ->closure(v | v.eContainer().name + ': ' + v.value + '\n').toUpperFirst()" labelPosition="node" resizeKind="NSEW">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelFormat>underline</labelFormat>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            </style>
          </subNodeMappings>
          <subNodeMappings name="ExcludeConstraint" semanticCandidatesExpression="aql:self.oclAsType(qml::Feature).eInverse('excludingFeature')" domainClass="qml::ExcludeConstraint">
            <style xsi:type="style:SquareDescription" labelExpression="aql:'Exclude: ' + self.excludedFeature.name->sep(', ')->toString() + ' ' + self.excludedAttribute.name + ' ' + self.excludedAttributeValue ->closure(v | v.eContainer().name + ': ' + v.value + '\n').toUpperFirst()" labelPosition="node" resizeKind="NSEW">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            </style>
          </subNodeMappings>
          <style xsi:type="style:FlatContainerStyleDescription" borderSizeComputationExpression="1" labelSize="12" widthComputationExpression="10">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
            <foregroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
          </style>
        </containerMappings>
        <toolSections name="Tools">
          <ownedTools xsi:type="tool:DirectEditLabel" name="editFeatureName">
            <mask mask="{0}"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="var:0"/>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:ContainerCreationDescription" name="CreateChild" precondition="aql:container.oclIsKindOf(qml::Feature)" containerMappings="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@containerMappings[name='FeatureContainer']" extraMappings="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@containerMappings[name='FeatureContainer']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="qml::Feature" referenceName="children"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:ContainerCreationDescription" name="Create Root" precondition="aql:self.oclAsType(qml::FeatureModel).root->size()=0" containerMappings="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@containerMappings[name='FeatureContainer']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="qml::Feature" referenceName="root"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="Create Open Attribute" precondition="aql:container.oclIsKindOf(qml::Feature)" nodeMappings="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@containerMappings[name='FeatureContainer']/@subNodeMappings[name='openAttribute']" extraMappings="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@containerMappings[name='FeatureContainer']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="qml::OpenValueAttribute" referenceName="attribute"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="Create Selection Attribute" nodeMappings="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@containerMappings[name='FeatureContainer']/@subNodeMappings[name='selectionAttribute']" extraMappings="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@containerMappings[name='FeatureContainer']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="qml::SelectionValueAttribute" referenceName="attribute"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="Create Quality Requirement" precondition="aql:container.qualityrequirement->isEmpty()" nodeMappings="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@nodeMappings[name='QualityRequirements']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="qml::QualityRequirements" referenceName="qualityrequirement"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:EdgeCreationDescription" name="Add Quality Requirement" forceRefresh="true" edgeMappings="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@edgeMappings[name='QualityRequirementEdge']">
            <sourceVariable name="source"/>
            <targetVariable name="target"/>
            <sourceViewVariable name="sourceView"/>
            <targetViewVariable name="targetView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:source">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="qml::QualityRequirement" referenceName="madeOf">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="relatedTo" valueExpression="var:target"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:EdgeCreationDescription" name="Add Threshold" forceRefresh="true" edgeMappings="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@edgeMappings[name='ThresholdEdge']">
            <sourceVariable name="source"/>
            <targetVariable name="target"/>
            <sourceViewVariable name="sourceView"/>
            <targetViewVariable name="targetView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:source.eInverse('relatedTo')->first()">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="qml::Threshold" referenceName="threshold">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="relatedTo" valueExpression="aql:target.eInverse('implementedBy')->first()"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
        </toolSections>
        <customization>
          <vsmElementCustomizations xsi:type="description:VSMElementCustomization" predicateExpression="aql:self.oclAsType(qml::Feature).mandatory">
            <featureCustomizations xsi:type="description:EAttributeCustomization" appliedOn="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@containerMappings[name='FeatureContainer']/@style" attributeName="labelFormat" value="bold"/>
          </vsmElementCustomizations>
          <vsmElementCustomizations xsi:type="description:VSMElementCustomization" predicateExpression="aql:self.oclAsType(qml::Feature).abstract">
            <featureCustomizations xsi:type="description:EReferenceCustomization" appliedOn="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@containerMappings[name='FeatureContainer']/@style" referenceName="backgroundColor">
              <value href="environment:/viewpoint#//@systemColors/@entries[name='light_gray']"/>
            </featureCustomizations>
          </vsmElementCustomizations>
          <vsmElementCustomizations xsi:type="description:VSMElementCustomization" predicateExpression="aql:view.targetNode.target.group.oclIsTypeOf(qml::OrGroup)">
            <featureCustomizations xsi:type="description:EAttributeCustomization" appliedOn="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@edgeMappings[name='ChildrenEdge']/@style" attributeName="lineStyle" value="dot"/>
            <featureCustomizations xsi:type="description:EAttributeCustomization" appliedOn="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@edgeMappings[name='ChildrenEdge']/@style/@centerLabelStyleDescription" attributeName="labelExpression" value="OR"/>
          </vsmElementCustomizations>
          <vsmElementCustomizations xsi:type="description:VSMElementCustomization" predicateExpression="aql:view.targetNode.target.group.oclIsTypeOf(qml::AltGroup)">
            <featureCustomizations xsi:type="description:EAttributeCustomization" appliedOn="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@edgeMappings[name='ChildrenEdge']/@style" attributeName="lineStyle" value="dash"/>
            <featureCustomizations xsi:type="description:EAttributeCustomization" appliedOn="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@edgeMappings[name='ChildrenEdge']/@style/@centerLabelStyleDescription" attributeName="labelExpression" value="ALT"/>
          </vsmElementCustomizations>
          <vsmElementCustomizations xsi:type="description:VSMElementCustomization" predicateExpression="aql:self.oclAsType(qml::QualityRequirements).madeOf.threshold.relatedTo.implementedBy">
            <featureCustomizations xsi:type="description:EReferenceCustomization" appliedOn="//@ownedViewpoints[name='FeatureModels']/@ownedRepresentations[name='FeaturesDiagram']/@defaultLayer/@containerMappings[name='FeatureContainer']/@style" referenceName="backgroundColor">
              <value href="environment:/viewpoint#//@systemColors/@entries[name='orange']"/>
            </featureCustomizations>
          </vsmElementCustomizations>
        </customization>
      </defaultLayer>
    </ownedRepresentations>
    <ownedJavaExtensions qualifiedClassName="it.disim.univaq.qml.design.Services"/>
  </ownedViewpoints>
</description:Group>
